{
    "name": "jms/di-extra-bundle",
    "type": "symfony-bundle",
    "description": "Allows to configure dependency injection using annotations",
    "keywords": ["annotations","dependency injection"],
    "homepage": "http://jmsyst.com/bundles/JMSDiExtraBundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ruudk/JMSDiExtraBundle",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/ruudk/JMSSecurityExtraBundle",
            "no-api": true
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "symfony/framework-bundle": "~2.7|~3.4|~4.0",
        "symfony/routing": "~2.7|~3.4|~4.0",
        "symfony/http-kernel": "~2.7|~3.4|~4.0",
        "symfony/dependency-injection": "~2.7|~3.4|~4.0",
        "symfony/process": "~2.7|~3.4|~4.0",
        "symfony/finder": "~2.7|~3.4|~4.0",
        "jms/aop-bundle": "~1.3",
        "jms/metadata": "~1.0"
    },
    "require-dev": {
        "symfony/validator": "~2.7|~3.4|~4.0",
        "symfony/form": "~2.7|~3.4|~4.0",
        "symfony/class-loader": "~2.7|~3.4|~4.0",
        "symfony/yaml": "~2.7|~3.4|~4.0",
        "symfony/browser-kit": "~2.7|~3.4|~4.0",
        "symfony/security-bundle": "~2.7|~3.4|~4.0",
        "symfony/expression-language": "~2.7|~3.4|~4.0",
        "symfony/twig-bundle": "~2.7|~3.4|~4.0",
        "sensio/framework-extra-bundle": "~2.0|~3.0|~4.0|~5.0",
        "jms/security-extra-bundle": "dev-symfony-4",
        "doctrine/doctrine-bundle": "~1.5",
        "doctrine/orm": "~2.3",
        "phpcollection/phpcollection": ">=0.2,<0.3-dev",
        "symfony/phpunit-bridge": "~3.4|~4.0",
        "phpunit/phpunit": "^4.8.35|^5.4.4|^6.0.0",
        "symfony/asset": "~2.7|~3.4|~4.0",
        "symfony/templating": "~2.7|~3.4|~4.0",
        "symfony/css-selector": "~2.7|~3.4|~4.0"
    },
    "autoload": {
        "psr-4": { "JMS\\DiExtraBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "JMS\\DiExtraBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    }
}
